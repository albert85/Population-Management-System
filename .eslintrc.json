{
  "extends": "airbnb-base",
  "settings": {
    "import/resolver": {
      "alias": [
        [
          "helpers",
          "./server/src/helpers"
        ],
        [
          "controllers",
          "./server/src/controllers"
        ],
        [
          "routes",
          "./server/src/routes"
        ],
        [
          "middlewares",
          "./server/src/middlewares"
        ],
        [
          "models",
          "./server/src/models"
        ]
      ]
    }
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "es6": true
    }
  },
  "plugins": [
    "import"
  ],
  "globals": {
    "window": true,
    "location": true,
    "localStorage": true,
    "expect": true,
    "jest": true,
    "document": true
  },
  "env": {
    "node": true,
    "es6": true,
    "mocha": true,
    "jquery": true
  }
}